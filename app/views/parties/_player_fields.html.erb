<div class='nested-fields'>
  <div class='field'>
    <%= f.label :name %>
    <%= f.text_field :name, placeholder: 'Player name, e.g. Henk' %>
  </div>

  <div class='field'>
    <%= f.label :level %>
    <%= f.select :level, 1..20 %>
  </div>

  <%= link_to_remove_association 'remove player', f %>
</div>