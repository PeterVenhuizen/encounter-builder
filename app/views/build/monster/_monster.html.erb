<% @monsters.each do |data| %>
  <% data.symbolize_keys! %>
  <% monster = Monster.new(data) %>
  <div class="player">
    <div class="emoji">&#128126;</div>
    <%# &#128128; %>
    <span class="name"><%= monster.name %></span>
    <div class="stats">
      <div>
        <label>CR: </label>
        <span><%= monster.cr %></span>
      </div>
      <div>
        <label>XP: </label>
        <span><%= monster.xp %></span>
      </div>
    </div>
    <%= link_to '&#10060;'.html_safe, "build/monster/#{data[:id]}", remote: true, method: :delete %>
  </div>
<% end %>